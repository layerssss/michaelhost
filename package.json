{
  "name": "michaelhost",
  "version": "0.9.2",
  "license": "MIT",
  "scripts": {
    "build-react": "react-app-rewired build",
    "build-pkg": "npx pkg ./bin/michaelhost.js --out-path ./dist --config ./package.json && cp ./node_modules/node-pty/build/Release/pty.node ./dist/pty.node",
    "build": "yarn build-react && yarn build-pkg",
    "prepack": "react-app-rewired build",
    "cli": "NODE_ENV=dev sudo `which node` ./bin/michaelhost.js --state-file-path /root/.michaelhost-state.json",
    "dev": "concurrently --kill-others --names \"react-app-rewired,node\" \"react-app-rewired start\" \"sudo NODE_ENV=dev `which node` ./bin/michaelhost.js --state-file-path /root/.michaelhost-state.json service --admin-port 4444 --email $EMAIL --webhook-server-port 4445 --secret notsuchasecret\"",
    "lint": "eslint .",
    "format": "prettier --write './lib/**/*.js' './bin/**/*.js' './src/**/*.js'"
  },
  "bin": {
    "michaelhost": "bin/michaelhost.js"
  },
  "dependencies": {
    "@root/acme": "^3.0.9",
    "@root/csr": "^0.8.1",
    "@root/keypairs": "^0.9.0",
    "@root/pem": "^1.0.4",
    "acme-http-01-webroot": "^3.0.0",
    "apollo-server-express": "^2.1.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-prettystream": "^0.1.3",
    "commander": "^2.15.1",
    "cookie": "^0.3.1",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "homedir": "^0.6.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-cron": "^2.0.3",
    "node-forge": "^0.9.1",
    "node-pty": "^0.8.1",
    "openid-client": "^2.4.5",
    "query-string": "^6.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "rollbar": "^2.5.2",
    "secure-compare": "^3.0.1",
    "semver": "^6.0.0",
    "shell-quote": "^1.6.1",
    "strip-ansi": "^5.0.0",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.0.0-beta.48",
    "@apollo/react-common": "^4.0.0-beta.1",
    "@apollo/react-hooks": "^4.0.0-beta.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@material-ui/core": "^4.9.14",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "concurrently": "^4.0.1",
    "customize-cra": "^0.9.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.1.3",
    "form-serialize": "^0.7.2",
    "graphql-tag": "^2.10.0",
    "http-proxy-middleware": "^0.19.0",
    "mdi-material-ui": "^6.14.0",
    "notistack": "^0.9.16",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.12.21",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-use-size": "^1.2.3",
    "roboto-fontface": "^0.10.0",
    "typeface-roboto": "^0.0.54",
    "xterm": "^3.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pkg": {
    "assets": [
      "./build/**/*"
    ]
  }
}
