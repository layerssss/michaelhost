{
  "name": "michaelhost",
  "version": "0.16.0",
  "license": "MIT",
  "scripts": {
    "build-react": "react-app-rewired build",
    "build": "yarn build-react && ./build_dist",
    "cli_sudo": "NODE_ENV=dev sudo `which node` ./bin/michaelhost.js --state-file-path /root/.michaelhost-state.json",
    "dev_sudo": "concurrently --kill-others --names \"react-app-rewired,node\" \"react-app-rewired start\" \"sudo NODE_ENV=dev `which node` ./bin/michaelhost.js --state-file-path /root/.michaelhost-state.json service --admin-port 2002 --email $EMAIL --webhook-server-port 2001 --secret ${MICHAELHOST_SECRET:-notsuchasecret}\"",
    "cli": "NODE_ENV=dev node ./bin/michaelhost.js",
    "dev": "concurrently --kill-others --names \"react-app-rewired,node\" \"react-app-rewired start\" \"NODE_ENV=dev node ./bin/michaelhost.js service --admin-port 2002 --secret ${MICHAELHOST_SECRET:-notsuchasecret} --email $EMAIL\"",
    "lint": "eslint .",
    "format": "prettier --write './lib/**/*.js' './bin/**/*.js' './src/**/*.js'"
  },
  "bin": {
    "michaelhost": "bin/michaelhost.js"
  },
