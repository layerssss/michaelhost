{
  "name": "michaelhost",
  "version": "0.8.13",
  "license": "MIT",
  "scripts": {
    "build-react": "react-scripts build",
    "build-pkg": "npx pkg ./bin/michaelhost.js --out-path ./dist --config ./package.json && cp ./node_modules/node-pty/build/Release/pty.node ./dist/pty.node",
    "build": "yarn build-react && yarn build-pkg",
    "prepack": "react-scripts build",
    "cli": "NODE_ENV=dev sudo `which node` ./bin/michaelhost.js --state-file-path /root/.michaelhost-state.json",
    "dev": "concurrently --kill-others --names \"react-scripts,node\" \"react-scripts start\" \"sudo NODE_ENV=dev `which node` ./bin/michaelhost.js --state-file-path /root/.michaelhost-state.json service --admin-port 4444 --email $EMAIL --webhook-server-port 4445 --secret notsuchasecret\"",
    "lint": "eslint .",
    "format": "prettier --write './lib/**/*.js' './bin/**/*.js' './src/**/*.js'",
    "test": "react-scripts test --env=jsdom"
  },
  "bin": {
    "michaelhost": "bin/michaelhost.js"
  },
  "dependencies": {
    "@root/acme": "^3.0.9",
    "@root/csr": "^0.8.1",
    "@root/keypairs": "^0.9.0",
    "@root/pem": "^1.0.4",
    "acme-http-01-webroot": "^3.0.0",
    "apollo-server-express": "^2.1.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-prettystream": "^0.1.3",
    "commander": "^2.15.1",
    "cookie": "^0.3.1",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "homedir": "^0.6.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-cron": "^2.0.3",
    "node-forge": "^0.9.1",
    "node-pty": "^0.8.1",
    "openid-client": "^2.4.5",
    "query-string": "^6.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "rollbar": "^2.5.2",
    "secure-compare": "^3.0.1",
    "semver": "^6.0.0",
    "shell-quote": "^1.6.1",
    "strip-ansi": "^5.0.0",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.4.7",
    "apollo-link": "^1.2.4",
    "apollo-link-error": "^1.1.2",
    "apollo-link-http": "^1.5.7",
    "bootstrap": "^3.4.1",
    "concurrently": "^4.0.1",
    "eslint-plugin-import": "^2.14.0",
    "form-serialize": "^0.7.2",
    "graphql-tag": "^2.10.0",
    "http-proxy-middleware": "^0.19.0",
    "jquery": "^3.3.1",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-apollo": "^2.2.4",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.4.1",
    "react-event-listener": "^0.6.4",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.0.5",
    "recompose": "^0.30.0",
    "roboto-fontface": "^0.10.0",
    "typeface-roboto": "^0.0.54",
    "xterm": "^3.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pkg": {
    "assets": [
      "./build/**/*"
    ]
  }
}
